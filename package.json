{
  "name": "xaas",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "description": "XSLT-transforms as a service on AMQP",
  "scripts": {
    "start": "./bin/start.js",
    "lint": "standard",
    "setup": "wget -O saxon.jar http://repo1.maven.org/maven2/net/sf/saxon/Saxon-HE/${VERSION:-9.9.0-1}/Saxon-HE-${VERSION:-9.9.0-1}.jar",
    "docker-build": "docker build -t lenkan/xaas:${TAG:-latest} .",
    "docker-push": "docker push lenkan/xaas:${TAG:?\"No TAG set\"}"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "tmp": "^0.0.33"
  },
  "devDependencies": {
    "standard": "^12.0.1"
  }
}
